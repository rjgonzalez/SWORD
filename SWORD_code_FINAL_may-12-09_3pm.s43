;------------------------------------------------------------------------------
;           Segment Configuration
;------------------------------------------------------------------------------
            RSEG    CSTACK                    ;pre-declaration of segment

;------------------------------------------------------------------------------
;           Code
;------------------------------------------------------------------------------
            RSEG    CODE                      ;place program in 'CODE' segment
RESET:
            mov.w #SFE(CSTACK),SP             ;Initialize stack pointer
            mov.w #WDTPW+WDTHOLD,&WDTCTL      ;Stop Watchdog Timer     
            call #SetSystemFrequency          ;Setup SMCLK = MCLK = DCO to 12MHz
            call #SetupEnv                    ;Setup device   
            call #InitializeVars
            call #Flush_Shift_Register  
            ;call #Fill_Shift_Register 
            ;call #Flush_Shift_Register
            bis.w #GIE, SR
            
main:      
            cmp.w #01h, &imgMode              ;Are we on image mode?
            jeq DisplayControl                      
            cmp.w #01, &txtBufRDY             ;If not, is txt buff RDY?            
            jne TextModeControl               ;If txt buff not rdy, process input. 
                                              ;If so, process incoming text and 
                                              ;display it!
;----------------------------------          
;        Display Handler                      
;-----------------------------------   
    DisplayControl:                           ;Proceed to begin displaying
            dint                              ;corresponding image or message.   
                        
    CheckIf_StorageIsNeeded:   
    
            nop
            nop
            cmp.w #0001h, &run_storage
                                              ;Do we need to storage next strip now?
            jne CheckIf_DisplayIsNeeded       ;Storing either a full 1 or full 0 strip
            call #Shift_NextStrip
            mov.w #0000h, &run_storage
            
    CheckIf_DisplayIsNeeded:                  ;Do we need to display now?
            cmp.w #0001h, &run_display        ;if so, toggle LE and let everyone
            jne Continue                      ;see the aweomse LED party!
            bis.b #BIT1, &P3OUT               ;Prendes el latch enable
            bic.b #BIT1, &P3OUT               ;Apago el latch enable
            mov.w #0001h, &run_storage
            mov.w #0000h, &run_display
            jmp Continue
            
;----------------------------------          
;        Text mode contro;                
;-----------------------------------             
            
    TextModeControl:                          ;Prepares everything to start displaying
            cmp.w #00h, &nextChar             ;user input. First, the msg is analyzed and
            jeq Continue
            call #AnalyzeMSG
            cmp.w #00h, &msgRDY
            jeq Continue
            call #PopulateTxtBuffer
            
        Continue:
            eint

            jmp main

;------------------------------------------------------------------------------
;           Interrupt Service Routines - START
;------------------------------------------------------------------------------
;#################################################################
; Interrupt Service Routine:  Port1_ISR
; Purpose: To reset the Display image index with the help of the optocoupler to allow 
;us have a steady image on SWORD
;#################################################################
Port1_ISR:
            mov.b #000b, &P1IFG
            mov.w #0C3Fh,&imgIndex
            ;xor.b #BIT0, &imgPointer
            reti       

;#################################################################
; Interrupt Service Routine:  TA0_ISR (Timer A0 Interrupt Service Routine)
;#################################################################
TA0_ISR:
            mov.w #01, &run_display   ;Enable display.
            reti            
            
;#################################################################
; Interrupt Service Routine:  UART_ISR 
; Purpose: Pushes the information in the RX Buffer to a repository array. This 
;          array will contain the entire message sent by the bluetooth. 
;#################################################################
UART_ISR:
            bic.b #UCRXIFG, &UCA1IFG 
            mov.w #0001h, &nextChar
            mov.w #0000h, &imgMode
            mov.w #0000h, &txtBufRDY
            reti             
            
;------------------------------------------------------------------------------
;           Subroutines - START
;------------------------------------------------------------------------------
;##################################################################
; Subroutine:  SetupEnv (Environment Setup)
; Purpose: Establishes system settings and timers.
; Pre-Condition: System Power On, or Reset
;##################################################################
SetupEnv:        

  SetPorts:
            bic.b   #0FFh, &P2OUT                     ;clear out RGB ports, 
            bic.b   #0FFh, &P3OUT                     ;LE, OE, CLK and RX pins.
            bic.b   #0FFh, &P4OUT
            bic.b   #0FFh, &P6OUT
            bic.b   #0FFh, &P1OUT
            bic.b   #0FFh, &P5OUT
            bic.b   #0FFh, &P8OUT
            bic.b   #0FFh, &P9OUT            
            bic.b   #0FFh, &P10OUT
   
            bic.b   #0FFh, &P7DIR                     ;clear out direction.
            bic.b   #0FFh, &P3DIR
            bic.b   #0FFh, &P4DIR
            bic.b   #0FFh, &P6DIR
            bic.b   #0FFh, &P1DIR
            bic.b   #0FFh, &P5DIR
            bic.b   #0FFh, &P8DIR
            bic.b   #0FFh, &P9DIR  
            bic.b   #0FFh, &P10DIR  

            bis.b   #0FFh, &P2DIR                     ;Set all ports to output.        
            bis.b   #0FFh, &P3DIR                 
            bis.b   #0FFh, &P4DIR                 
            bis.b   #0FFh, &P6DIR
            bic.b   #0FFh, &P1DIR
            bis.b   #0FFh, &P5DIR
            bis.b   #0FFh, &P8DIR
            bis.b   #0FFh, &P9DIR
            bis.b   #0FFh, &P10DIR     
  
  SetupOpto:
            bis.b #BIT4, &P1IES   ; Set the edge from High to Low
            bic.b #BIT4, &P1IFG   ; Clear the IFG
            bis.b #BIT4, &P1IE    ; Allow the Interrupt
            
  SetupUART:  
            bic.b   #BIT7, &P5DIR                     ;Except UART, which wil be 
                                                      ;reading information.             
            mov.b #00C0h, P5SEL
            bis.b #UCSSEL_1, &UCA1CTL1
            mov.b #0x03, &UCA1BR0 
            mov.b #0x00, &UCA1BR1 
            bis.b #UCBRS1 + UCBRS0, &UCA1MCTL 
            bic.b #UCSWRST, &UCA1CTL1
            bis.b #UCRXIE, &UCA1IE              
           
  SetupTA0:
	    mov.w #5402d, &TA0CCR0                     ;Set time on timer
            bis.w #CCIE, &TA0CCTL0                    ;Compare-mode interrupt.
	    mov.w #TACLR+ID_0+MC_1+TASSEL_2,&TA0CTL   ;up mode, ACLK
                       
  SetupImage:
  
            bic.b #BIT0, &P2DIR                       ; Set to Input
            push.w R6
            mov.b &P2IN, R6
            clrc
            rrc.b R6     ;Rotate the P1 In
            
            ;Check Carry
            
            jc setOneImage              ; If zero, set the imgIndex to 0
            mov.w #0000h, &imgPointer    
            jmp finishSetup
            
            setOneImage:
            mov.w #0001h, &imgPointer
            
            finishSetup:
            pop.w R6
 
;##################################################################
; Subroutine:  InitializeVars
;##################################################################  
InitializeVars:           
 
;*******IMPORTANT********** 
; The following are the correct initial values
;the ones above are only meant for debugging. 
;please delete them and un comment these once you
;are done debugging.
            mov.w #0000h, &run_storage
            mov.w #0000h, &run_display
            mov.w #0C3Fh, &imgIndex
            ;mov.w #0000h, &imgPointer
            mov.w #0001h, &imgMode
            mov.w #0000h, &nextChar
            mov.w #0000h, &msgRDY
            mov.w #0000h, &txtBufRDY   
            
            ret            
        
        
;##################################################################
; Subroutine:  Flush_Shift_Register, Debugging Purposes
;##################################################################     
Flush_Shift_Register:
            push.w R7
            clr.w R7
            bic.b #BIT0, &P3OUT   ; Apagas el clock
            bic.b #31d, &P10OUT   ; Pones un cero en la salida
            bic.b #31d, &P9OUT
            bic.b #31d, &P8OUT
            bic.b #BIT1, &P6OUT   ; Prendes el output enable
flush_this:
            bis.b #BIT0, &P3OUT   ; Prendes el clock
            bic.b #BIT0, &P3OUT   ; Apagas el clock                      
            cmp #010H, R7
         
            jeq end_flush_routine 
            inc.w R7
            jmp flush_this
end_flush_routine:
            bis.b #BIT1, &P3OUT    ;Prendes el latch enable
            bic.b #BIT1, &P3OUT    ;Prendes el latch enable
            pop.w R7
            
            ret
           
;##################################################################
; Subroutine:  Fill_Shift_Register , Debugging Purposes
;##################################################################     
Fill_Shift_Register:
            push.w R7
            clr.w R7
            bic.b #BIT0, &P3OUT   ; Apagas el clock
            bis.b #31d, &P10OUT   ; Pones un uno en la salida 
            bic.b #BIT1, &P6OUT   ; Prendes el output enable
fill_this:
            bis.b #BIT0, &P3OUT   ; Prendes el clock
            bic.b #BIT0, &P3OUT   ; Apagas el clock                      
            cmp #010H, R7
         
            jeq end_fill_routine 
            inc.w R7
            jmp fill_this
end_fill_routine:
            bis.b #BIT1, &P3OUT    ;Prendes el latch enable
            bic.b #BIT1, &P3OUT    ;Prendes el latch enable
            pop.w R7
            
            ret           
           
           
;##################################################################
; Subroutine:  ZeroStrip_Shift_Register
;##################################################################             
Shift_NextStrip:
            push.w R5
            push.w R7
            push.w R8
            push.w R9
            push.w R10            
            clr.w R7
            clr.w R8
            clr.w R9
            clr.w R10
            
            bic.b #BIT0, &P3OUT            ;Apagas el clock 
            bic.b #BIT1, &P6OUT            ;Prendes el output enable
            
        KeepShifting:
            cmp #010H, R7
            jeq NextSrtip_isDone
            mov.w &imgIndex, R8
            mov.w &imgIndex, R9
            mov.w &imgIndex, R10
           
            cmp.w #00h, &imgMode           ;Are we on image mode?
            jne ShiftImg   
;            mov.b txtBuff(R10), &P8OUT      ;If not, shift textbuffer. 
            mov.b txtBuff(R10), &P8OUT      ;If not, shift textbuffer. 
            jmp ShiftIt
            
            ShiftImg:
            cmp.w #0000h, &imgPointer      ;If so, shift img0 or img1?
            jne ShiftImg1    
            mov.b imgRED0(R8), &P9OUT
            mov.b imgGREEN0(R9), &P10OUT
            mov.b imgBLUE0(R10), &P8OUT    ;Sets the next bit in SDI for R,G & B SRs.
            jmp ShiftIt
            
        ShiftImg1:
            mov.b txtBuff(R10), R5
            mov.b imgRED1(R8), &P9OUT
            mov.b imgGREEN1(R9), &P10OUT
            mov.b imgBLUE1(R10), &P8OUT    ;Sets the next bit in SDI for R,G & B SRs.
            
        ShiftIt:            
            bis.b #BIT0, &P3OUT            ;Toggle the clock, Shifting!!!
            bic.b #BIT0, &P3OUT            
                       
            inc.w R7                       ;Next bit!
            dec.w &imgIndex                ;Next byte!, We need the corresponding
            jmp KeepShifting               ;bit to the corresponding SR.
            
        NextSrtip_isDone:
        
            cmp.w #000h, &imgIndex         ;Due to our motor's rotation direction
            jge NoIndex_ResetNeeded        ;we are iterating backwards to achieve    
            mov.w #0C3Fh, &imgIndex        ;correct display. 
        
        NoIndex_ResetNeeded:
        
            pop.w R10
            pop.w R9            
            pop.w R8
            pop.w R7
            pop.w R5         
            ret   
            
;##################################################################
; Subroutine:  AnalyzeMSG
;##################################################################

AnalyzeMSG:
        push.w R5                   ;To use it as a temp var.
        push.w R7
        push.w R8
  CheckIfBack2Img:
        cmp.b #023h, &UCA1RXBUF     ;Is the user requesting image 0?
        jne CheckIfImg1				;Using reserved char "#"
        mov.w #00h, &imgPointer
        jmp SetImgFlags
	CheckIfImg1:	        
        cmp.b #024h, &UCA1RXBUF     ;Is the user requesting image 1?
        jne CheckIfNewMessage		;Using reserved char "$"
        mov.w #01h, &imgPointer    
	SetImgFlags:
        mov.w #01, &imgMode         ;If so, set imgMode flag and reset index.
        mov.w #0C3Fh, &imgIndex
        jmp Done4Now        
        
  CheckIfNewMessage:                ;If not, check if the user is requesting to
        cmp.b #02Ah, &UCA1RXBUF     ;display a new message. 
        jne CheckIfMsgIsDone
        call #Flush_Shift_Register        ;later is pushed to a text buffer. 
        mov.w #00h, &imgMode         ;We are currently on txtMode
        mov.w #0C3Fh, &imgIndex     ;New message, reset index.  
        
    CheckIfClear:        
        cmp.w #019d, &msgIndex      ;Clear msgArray for new user input.    
        jeq MSGArrayIsCleared       
        mov.w &msgIndex, R8          
        mov.b #03Ah, msg(R8)
        inc.w &msgIndex
        jmp CheckIfClear
    
    MSGArrayIsRDY:  
        mov.w #01h, &msgRDY
    MSGArrayIsCleared:
        mov.w #00h, &msgIndex
        jmp Done4Now
              
  CheckIfMsgIsDone:
        cmp.b #02Eh, &UCA1RXBUF       ;Has the msg been transmitted 
        jne PushNextChar             ;completely?
        mov.w #00h, &msgIndex
         
    CheckIfRDY:        
        cmp.w #019d, &msgIndex       ;Have we processed all chars?
        jeq MSGArrayIsRDY            ;If so, we are done. 
        mov.w &msgIndex, R8          ;If not, process next char.
        cmp.w #00h, msg(R8)          ;Since we wont display all ASCII
        jeq NoFixNeeded              ;chars we need to fix some in order
                                      ;to use properly our char LUT. 
        mov.b msg(R8), R5             ;***** DEBUGGING PURPOSES
        cmp.b #061h, msg(R8)
        jlo ItsANumber
        sub.b #061h, msg(R8)
        jmp NextChar
    ItsANumber:
        sub.b #016h, msg(R8)
    NextChar:
    NoFixNeeded:
        inc.w &msgIndex                ;inc and process next char.
        jmp CheckIfRDY
                
    PushNextChar:
        mov.w &msgIndex, R8
        mov.b &UCA1RXBUF, msg(R8)    ;push next char to our msg array.
        inc.w &msgIndex
    Done4Now:
        mov.w #00h, &nextChar
        pop.w R8
        pop.w R7
        pop.w R5
        
        ret            
            
;##################################################################
; Subroutine:  PopulateTxtBuffer
;##################################################################               
PopulateTxtBuffer:
        push.w R5             ;Also for debugging purposes
        push.w R8             ;Counter
        push.w R9             ;Current Char Start_Position
        push.w R10            ;txtBuffer Index (tbi)
        push.w R11            ;CurrentCharHolder
        push.w R12            ;textBuffer Index Interlaced (tbii)
        push.w R13            ;Strip Monitor Counter
        push.w R14            ;Debugging purposes
        
        mov.w #00h, R8        ;Clearing our variable-registers. 
        mov.w #00h, R9
        mov.w #00h, R10
        mov.w #00h, R11
        mov.w #00h, R12
        mov.w #00h, R13
        mov.w #00h, R14 
        mov.w #00h, R5
        
        
    TxtBuff_NotClrYet:    
        cmp.w #0C3Fh, R5
        jeq TxtBuff_isClear
        mov.b #00h, txtBuff(R5)
        inc.w R5
        jmp TxtBuff_NotClrYet
        
    TxtBuff_isClear:    
        
        mov.b #02h, &LD     ;Letter Destination is currently on SR3
        mov.b &LD, R5       ;Debugging... 
        ;mov.b #02h, &LDI    ;Letter Destination Interlaced is currently on SR1
        ;mov.b &LDI, R5      ;Debugging...
        
        mov.w #00h, R5
        
        mov.w #0610h, R12     ;Initializing the tbii.
        mov.w #00h, &msgIndex ;Clearing the msgIndex, we are starting!
        
        
    Push_Nxt_Char:        
        cmp.w #019d, &msgIndex
        jeq TxtBufferRDY
      
    Is_Offset_RDY: 
        mov.w &msgIndex, R11
        ;mov.w msg(R11), R14  ;Debugging purposes, keep track of current char.
        cmp.b msg(R11), R8
        jeq OffsetRDY         ;Since we dont have MPY instruction I am adding
        add.w #100d, R9         ;the offset x times, x = msg[i].
        inc.w R8
        jmp Is_Offset_RDY   
        
    OffsetRDY:    
        mov.w #00h, R8        ;Clear counter, StartPosition( = R9) is rdy.    
        
   CurrentLetterIsNotDone:     
        cmp.w #100d, R8        ;Have I copied all 100 bits of current char?
        jeq Nxt_Char
        
      NextByte:
        mov.b charRep(R9), R5
        cmp.b #01h, charRep(R9)
        
        jne Nothing_to_Set
        ;jne Check_if_Interlaced
        bis.b &LD, txtBuff(R10)
      ;Check_if_Interlaced:
      ;  inc.w R9
      ;  mov.b charRep(R9), R5
      ;  cmp.b #01h, charRep(R9)
      ;  jne Nothing_to_Set
        ;bis.b &LDI, txtBuff(R12)
             
      Nothing_to_Set:  
            
        inc.w R8                ;Inc counter. Determines if we 
                                ;are done pushing current char.
        inc.w R9                ;Inc startposition, to select
                                ;next value that goes into txtbuff.
        inc.w R10               ;Keeps track of correct position 
        ;cmp.w #060Dh, R10
        ;jeq TBII_ResetNeeded         
        ;inc.w R12               ;to place next value in txtbuff.
        ;jmp ContinueInc
        
    ;TBII_ResetNeeded:
    ;    mov.w #0000h, R12
    ;    mov.w #0002h, &LD     ;Swaping , we have reached the middle of SWORD.
    ;    mov.w #0008h, &LDI 
        
    ContinueInc:   
        inc.w R13               ;Strip Monitor (SM), when reaches 5, we have
                                ;cover one entire strip. Next strip!.
        cmp.w #0Ah, R13         ;Has the SM reached 5?                        
        jne NextByte                        
        mov.w #0000h, R13 
        
        add.w #06h, R10       ;Setting the correct shifting location for tbi
        add.w #06h, R12       ;Setting the correct shifting location for tbii
        
        jmp CurrentLetterIsNotDone
    
    Nxt_Char:
        inc.w &msgIndex
        mov.w #00h, R9        ;Reset Start position, will be set correctly later. 
        mov.w #00h, R8        ;Clear counter for next char. 
        mov.w #00h, R13
        jmp Push_Nxt_Char
     
    TxtBufferRDY:
        mov.w #00h, &msgIndex             ;TxtBuffer is RDY, clear 
        mov.w #00h, &msgRDY               ;everything and set buffer flag to 
        mov.w #01h, &txtBufRDY            ;shift it!
        pop.w R8
        pop.w R9 
        pop.w R10
        pop.w R11 
        pop.w R12
        pop.w R13
        pop.w R14
        pop.w R5
        
        ret            
            
            
;##################################################################
; Subroutine:  ZeroStrip_Shift_Register
;##################################################################             
SetSystemFrequency:

            bis.w #0x0000, &UCSCTL0
            bis.w #DCORSEL_6, &UCSCTL1
            mov.w #0x0176, &UCSCTL2
            bis.w #SELREF_2, &UCSCTL3
            mov.w #SELS_3+SELM_3, &UCSCTL4  
            ret            
           
            END                    ;End of application.

                     